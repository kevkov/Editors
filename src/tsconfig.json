{
    "compilerOptions": {
        "module": "ES2020",
        "moduleResolution": "Node",
        "target": "ES2020",
        "esModuleInterop": true,
        "lib": [
            "ES2020",
            "DOM",
            "DOM.Iterable"
        ],
        "jsx": "react-jsx",
        "strict": true,
        "strictNullChecks": true,
        "resolveJsonModule": true,
        "isolatedModules": true,
        "useDefineForClassFields": true
    },
    "exclude": [
        "node_modules"
    ]
}